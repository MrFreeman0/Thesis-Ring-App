<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:height="512dp"
    android:width="512dp"
    android:viewportHeight="1024"
    android:viewportWidth="1024">
    <item android:width="100dp">

        <shape
            android:shape="oval"
            android:useLevel="true">
            <padding
                android:right="0dp"
                android:top="0dp" />
            <size
                android:width="64dp"
                android:height="64dp" />
            <gradient
                android:startColor="@color/colorOuterRingStart"
                android:endColor="@color/colorOuterRingEnd"
                android:type="linear"
                android:angle="45" />
        </shape>

    </item>
    <item
        android:width="96dp"
        android:height="96dp"
        android:left="2dp"
        android:top="2dp"
        android:bottom="2dp"
        android:right="2dp">
        <shape
            android:shape="oval"
            android:useLevel="true">
            <gradient
                android:useLevel="true"
                android:startColor="@color/colorGradientStart"
                android:endColor="@color/colorGradientEnd"
                android:type="linear"
                android:angle="45" />
        </shape>
    </item>
</layer-list>